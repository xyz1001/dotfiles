[core]
    autocrlf = false
    safecrlf = false
	editor = nvim
[color]
    ui = true
[help]
    autocorrect = -1
[i18n]
    commitencoding = utf-8
    logoutputencoding = utf-8
[gui]
    encoding = utf-8
[pull]
    rebase = true
[include]
    path = ~/.gitconfig.user
    path = ~/.gitconfig.diff
    path = ~/.gitconfig.proxy
[credential "helperselector"]
	selected = manager-core
